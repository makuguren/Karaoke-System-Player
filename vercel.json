{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "https://api-dcoderzoke.vercel.app/api/$1"
      }
    ],
    "routes": [
      {
        "src": "/(.*\\.(js|css|svg|png|jpg|jpeg|gif|ico|json|woff2))",
        "headers": {
          "cache-control": "public, max-age=31536000, immutable"
        },
        "dest": "/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }